    "http-status": "^1.7.3",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0"
